<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.swisscom.regio.controller.TournamentListController">
   <children>
      <ImageView fitHeight="139.0" fitWidth="599.0" pickOnBounds="true" preserveRatio="true" />
      <TableView fx:id="tournamentTable" prefHeight="210.0" />
      <HBox alignment="CENTER" prefHeight="69.0" prefWidth="600.0">
         <children>
            <Button mnemonicParsing="false" onAction="#openOverview" text="View" />
            <Button mnemonicParsing="false" onAction="#createTournament" text="+ Tournament">
               <HBox.margin>
                  <Insets left="300.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
